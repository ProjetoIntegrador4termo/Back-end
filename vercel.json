{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "node_version": "18",
                "npmRunBuild": "install && npm start"
            }
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "index.js"
        }
    ]
}
